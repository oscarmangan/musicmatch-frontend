 valid\n        if(username === '' || password === ''){\n            alert('Username/Password cannot be empty!');\n            return;\n        }\n\n        fetch(this.state.HOST + 'auth/', {\n            async: true,\n            method: 'POST',\n            headers: {\n                'Content-Type': 'application/json'\n            },\n            body: JSON.stringify({\n                username: username,\n                password: password\n            })\n        }).then(response => {\n            if(!response.ok){\n                throw Error(response.statusText);\n            } else {\n                return response.json();\n            }\n        }).then(json => {\n            localStorage.setItem('token', json.token);\n            this.setState({\n                isAuthorised: true\n            });\n        }).catch((error) => {\n            alert(error);\n            throw error;\n        })\n    };\n\n    /*\n    Logout functionality, the token is cleared from local storage. The state is reset to\n    being unauthorised and the application is closed off from the user\n     */\n    logout = () => {\n        localStorage.clear();\n        this.setState({\n            isAuthorised: false\n        })\n    }\n\n    /*\n    Render() where components are displayed along with routing SPA functionality\n     */\n    render() {\n        return (\n            <BrowserRouter>\n                {!this.state.isAuthorised ? <Redirect to='/login'/> : <Redirect to='/home'/>}\n                    <Fragment>\n                        <MenuBar auth={this.state.isAuthorised} logout={this.logout}/>\n                    </Fragment>\n                    <div className=\"pageContent\">\n                        <Switch>\n                            <Route path='/home'>\n                                <Homepage />\n                            </Route>\n                            <Route path='/discover'>\n                                <Discover />\n                            </Route>\n                            <Route path='/profile'>\n                                <Profile\n                                    uname={this.state.username}\n                                    email={this.state.email}\n                                />\n                            </Route>\n                            <Route path='/search'>\n                                <Search />\n                            </Route>\n                            <Route path='/login'>\n                                <LoginForm\n                                    onClick={this.login}\n                                />\n                            </Route>\n                            <Route path='/signup'>\n                                <Signup/>\n                            </Route>\n                        </Switch>\n                    </div>\n            </BrowserRouter>\n        );\n    }\n}\n\nexport default App;\n","/Users/oscarmangan/WebstormProjects/musicmatch/src/components/menuBar.jsx",[],"/Users/oscarmangan/WebstormProjects/musicmatch/src/components/loginForm.jsx",[],"/Users/oscarmangan/WebstormProjects/musicmatch/src/index.js",[],["78","79"],"/Users/oscarmangan/WebstormProjects/musicmatch/src/components/signupOne.jsx",[],"/Users/oscarmangan/WebstormProjects/musicmatch/src/components/signupTwo.jsx",[],"/Users/oscarmangan/WebstormProjects/musicmatch/src/components/instrumentsForm.jsx",[],"/Users/oscarmangan/WebstormProjects/musicmatch/src/components/genresForm.jsx",[],"/Users/oscarmangan/WebstormProjects/musicmatch/src/components/homepage.jsx",[],"/Users/oscarmangan/WebstormProjects/musicmatch/src/components/discover.jsx",["80"],"import React, { Component, Fragment } from 'react';\nimport mmIcon from '../static/mm_icon_gradient.png';\n\nclass Discover extends Component {\n    state = {}\n\n    render() {\n        return(\n            <Fragment>\n                <h2>Discover</h2>\n\n            </Fragment>\n        )\n    }\n}\n\nexport default Discover;"